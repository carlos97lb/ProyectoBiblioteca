<Window x:Class="ProyectoBiblioteca.clases.DialogoCatalogo"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ProyectoBiblioteca.clases"
        mc:Ignorable="d"
        Title="DialogoCatalogo" Height="450" Width="800">
    <Grid>
        <TextBox x:Name="Textid" HorizontalAlignment="Left" Height="23" Margin="389,114,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120"/>
        <Label Content="ID:" HorizontalAlignment="Left" Margin="307,114,0,0" VerticalAlignment="Top"/>
        <Button x:Name="botonaceptar" Content="ACEPTAR" HorizontalAlignment="Left" Margin="439,225,0,0" VerticalAlignment="Top" Width="75" Click="botonaceptar_Click"/>
        <Button x:Name="botoncancelar" Content="CANCELAR" HorizontalAlignment="Left" Margin="317,225,0,0" VerticalAlignment="Top" Width="75" Click="botoncancelar_Click"/>

    </Grid>
</Window>
